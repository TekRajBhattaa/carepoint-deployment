{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "verdure/vdslider-item",
    "version": "0.1.0",
    "title": "VD-Slider Item",
    "description": "VD-Slider slide item block, used with vdslider block.",
    "category": "vdplug",
    "parent": [
        "verdure/trusted-slider"
    ],
    "supports": {
        "html": false,
        "anchor": true
    },
    "attributes": {
        "allowedBlocks": {
            "type": "array",
            "enum": [
                "verdure/vdblock",
                "core/group",
                "core/columns"
            ]
        },
        "templateLock": {
            "type": [
                "string",
                "boolean"
            ],
            "enum": [
                "all",
                "insert",
                false
            ]
        }
    },
    "textdomain": "vdplug",
    "editorScript": "file:index.js"
}